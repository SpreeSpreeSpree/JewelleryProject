<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
   version="2.0">
	<managed-bean>
		<managed-bean-name>customer</managed-bean-name>
		<managed-bean-class>org.jewellery3.Customer</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customerController</managed-bean-name>
		<managed-bean-class>org.jewellery3.CustomerController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customerClient</managed-bean-name>
		<managed-bean-class>org.jewellery3.CustomerClient</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
		<managed-bean>
		<managed-bean-name>employee</managed-bean-name>
		<managed-bean-class>ewel.Employee</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>employeManager</managed-bean-name>
		<managed-bean-class>ewel.EmployeManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>employeController</managed-bean-name>
		<managed-bean-class>ewel.EmployeController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>payroll</managed-bean-name>
		<managed-bean-class>ewel.Payroll</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>product</managed-bean-name>
		<managed-bean-class>product.Product</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>productManager</managed-bean-name>
		<managed-bean-class>product.ProductManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>productController</managed-bean-name>
		<managed-bean-class>product.ProductController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
		<managed-bean>
		<managed-bean-name>home</managed-bean-name>
		<managed-bean-class>jewelery.Home</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
</faces-config>